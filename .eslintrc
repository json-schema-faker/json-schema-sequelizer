{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules" : {
    "strict": 0,
    "no-console": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0
  }
}
