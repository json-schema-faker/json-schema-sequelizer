    "sqlite3": "^3.1.8"
  },
  "dependencies": {
    "fs-extra": "^4.0.2",
    "graphql": "^0.11.7",
    "graphql-sequelize": "^5.5.0",
    "graphql-tools": "^2.7.2",
    "json-schema-faker": "^0.5.0-rc11",
