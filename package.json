    "pg": "^7.4.0",
    "pg-native": "^2.2.0",
    "sqlite3": "^4.0.0"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "json-schema-ref-parser": "^5.0.0",
    "sequelize": "^4.22.5",
    "umzug": "^2.0.1"
